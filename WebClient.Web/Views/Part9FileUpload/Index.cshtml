@model IEnumerable<ClientWeb.Models.Part9InexView>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FileURI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileRemarks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileDateStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FileUserStamp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FileURI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileRemarks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileDateStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FileUserStamp)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
